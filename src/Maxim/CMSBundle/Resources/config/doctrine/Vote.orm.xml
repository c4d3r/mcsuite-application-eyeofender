<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Maxim\CMSBundle\Entity\Vote" table="vote">

    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>

    <field name="name" type="string" column="name" length="45" nullable="true"/>
    <field name="link" type="text" column="link" nullable="true"/>
    <field name="image" type="string" column="image" length="45" nullable="true"/>
    <field name="reset" type="integer" column="reset" nullable="true"/>
    <field name="website" type="string" column="website" length="45" nullable="true"/>
    <field name="votifier" type="boolean" column="votifier" nullable="true"/>

  </entity>
</doctrine-mapping>
